                <script>
                    var osmUrl = 'https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png',
                        osmAttrib = '&copy; <a href="http://openstreetmap.org/copyright">OpenStreetMap</a> contributors',
                        osm = L.tileLayer(osmUrl, {maxZoom: 17, attribution: osmAttrib});
                    <!-- 41.38822581428823, 2.1133245001212693 -- coords de la fib -->
                    var coords_x = 41.38822581428823;
                    var coords_y = 2.1133245001212693;

                    var tag = "FIB UPC";

                    var map = L.map('map').setView([coords_x, coords_y], 17).addLayer(osm);
                    L.marker([coords_x, coords_y])
                        .addTo(map)
                        .bindPopup('FIB UPC.')
                        .openPopup();

                </script>

